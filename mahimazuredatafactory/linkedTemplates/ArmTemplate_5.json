					},
					{
						"name": "ForEach1",
						"type": "ForEach",
						"dependsOn": [
							{
								"activity": "project_gc",
								"dependencyConditions": [
									"Succeeded"
								]
							},
							{
								"activity": "project copy sql gc",
								"dependencyConditions": [
									"Succeeded"
								]
